int ThermistorPin = 0;
int Vo;
float R1 = 9940;
float logR2, R2, T;
float c1 = 0.000650333285226053, c2 = 0.000780888672114694, c3 = -9.25430040536406e-06;

void setup() {
Serial.begin(9600);
}

void loop() {

  Vo = analogRead(ThermistorPin);
  R2 = R1 * (1023.0 / (float)Vo - 1.0);
  logR2 = log(R2);
  T = (1.0 / (c1 + c2*logR2 + c3*logR2*logR2*logR2));
  T = T - 273.15;
 
  Serial.print("Temperature: "); 
  Serial.print(T);
  Serial.println(" C"); 

  delay(500);
}
